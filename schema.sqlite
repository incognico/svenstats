CREATE TABLE stats(
   steamid64     integer   primary key,
   steamid       text      unique,
   name          text,
   id            integer,
   score         real,
   lastscore     real,
   deaths        integer,
   lastdeaths    integer,
   scoregain     real,
   deathgain     integer,
   joins         integer,
   geo           text,
   lat           real,
   lon           real,
   datapoints    integer,
   datapointgain integer,
   seen          text,
   first         text
);
CREATE TABLE maps(
   map           text      primary key,
   count         integer
);
CREATE TABLE misc(
   hold          integer
);
